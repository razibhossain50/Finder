{
    "private": true,
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "dev": "npm run dev:frontend & npm run dev:backend",
        "dev:frontend": "npm --workspace frontend run dev",
        "dev:backend": "npm --workspace backend run dev",
        "build": "npm run build:frontend && npm run build:backend",
        "build:frontend": "npm --workspace frontend run build",
        "build:backend": "npm --workspace backend run build",
        "start": "npm run start:frontend & npm run start:backend",
        "start:frontend": "npm --workspace frontend run start",
        "start:backend": "npm --workspace backend run start",
        "test": "npm run test:frontend && npm run test:backend",
        "test:biodata": "npm run test:biodata:frontend && npm run test:biodata:backend && npm run test:biodata:e2e",
        "test:biodata:frontend": "npm --workspace frontend run test:frontend",
        "test:biodata:backend": "npm --workspace backend run test:biodata",
        "test:biodata:e2e": "npm --workspace frontend run test:e2e",
        "test:biodata:api": "npm --workspace backend run test:api",
        "test:frontend": "npm --workspace frontend run test",
        "test:backend": "npm --workspace backend run test"
    }
}